dag1 <- function()
{
f<-scan(file = "Input1.txt", what = integer())
increments <- 0
for (i in 2:length(f))
{
previous <- f[i - 1]
current <- f[i]
if (current > previous)
{
increments <- increments + 1
}
}
return(increments)
}
dag1()
